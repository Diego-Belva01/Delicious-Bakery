<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- This Menu Item will appear in the Upper bar, that's why It needs NO parent or action -->
    <menuitem
        id="deliciousbakery_menu_root"
        name="Delicious Bakery"
        sequence="10"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem
        id="deliciousbakery_roti_menu_categ"
        name="Roti"
        parent="deliciousbakery_menu_root"
        sequence="10"/>
    
    <!-- This Menu Item Must have a parent -->
    <menuitem
        id="deliciousbakery_pegawai_menu_categ"
        name="Pegawai"
        parent="deliciousbakery_menu_root"
        sequence="20"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem
	    id="deliciousbakery_transaksi_menu_categ"
	    name="Transaksi"
	    parent="deliciousbakery_menu_root"
	    sequence="42"/>

</odoo>