<odoo>

    <!-- Report Supplier -->
    <record id="report_supplier_xlsx" model="ir.actions.report">
        <field name="name">Supplier Excel</field>
        <field name="model">deliciousbakery.supplier</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">deliciousbakery.report_supplier_xlsx</field>
